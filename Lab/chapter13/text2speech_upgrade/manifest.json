{
 "manifest_version": 3,
 "name": "Text to Speech 2",
 "version": "1.0",
 "permissions": ["tabs","activeTab", "scripting","tts","downloads"],
 "action": {
   "default_popup": "popup.html",
   "default_icon": {
     "16": "images/icon16.png",
     "48": "images/icon48.png",
     "128": "images/icon128.png"
   }
 },
 "background": {
   "service_worker": "background.js"
 }
 
}

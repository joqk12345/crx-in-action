{
  "name": "ctx-vue-app",
  "version": "1.0",
  "description": "build chrome extention base vue & vite5 demo",
  "manifest_version": 3,
  //   "background": {
  //     "service_worker": "background.js"
  //   },
  "content_scripts": [
    {
      "matches": ["https://*/*"],
      "css": ["content.css"],
      "js": ["content.js"],
      "run_at": "document_end"
    }
  ],
  "host_permissions": ["https://*/*"],
  "action": {
    "default_popup": "index.html",
    "default_icon": {
      "16": "images/app.png",
      "32": "images/app.png",
      "48": "images/app.png",
      "128": "images/app.png"
    }
  },
  "icons": {
    "16": "images/app.png",
    "32": "images/app.png",
    "48": "images/app.png",
    "128": "images/app.png"
  },
  "permissions": ["storage", "declarativeContent"]
}

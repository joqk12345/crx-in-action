{
  "manifest_version": 3,
  "name": "My First  Content Script Extension",
  "version": "1.0",
  "permissions": ["activeTab", "scripting"],
  "host_permissions": ["https://*/*"],
  "background": {
    "service_worker": "background.js"
  },
  "action": {},
  "content_scripts": [
    {
      "matches": ["https://*/*"],
      "js": ["content.js"]
    }
  ]
}
